# Instagram Configuration (Optional - for authenticated access)
INSTAGRAM_USERNAME=your_instagram_username
INSTAGRAM_PASSWORD=your_instagram_password

# Apple Music API Configuration
# Get these from https://developer.apple.com/account/
APPLE_TEAM_ID=your_team_id
APPLE_KEY_ID=your_key_id
APPLE_PRIVATE_KEY_PATH=path/to/AuthKey_XXXXX.p8

# Or alternatively, use a pre-generated developer token
# APPLE_DEVELOPER_TOKEN=your_developer_token

# Apple Music User Token (generated through MusicKit JS)
APPLE_MUSIC_USER_TOKEN=your_user_token

# Script Configuration
TARGET_INSTAGRAM_USER=username_to_scrape
MAX_POSTS=100
DRY_RUN=false
OUTPUT_ONLY=false  # Set to true to only scrape and save to file, skip Apple Music
LOG_LEVEL=INFO
